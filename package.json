{
  "name": "todo-taskpaper",
  "version": "0.0.1",
  "description": "Enhanced text editor for TaskPaper files.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "phosphor-arrays": "^1.0.6",
    "phosphor-dockpanel": "^0.9.7",
    "phosphor-domutil": "^1.2.0",
    "phosphor-dragdrop": "^0.9.0",
    "phosphor-messaging": "^1.0.6",
    "phosphor-nodewrapper": "^1.0.6",
    "phosphor-properties": "^2.0.0",
    "phosphor-splitpanel": "^1.0.0-rc.1",
    "phosphor-stackedpanel": "^1.0.0-rc.0",
    "phosphor-tabs": "^1.0.0-rc.2",
    "phosphor-widget": "^1.0.0-rc.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "css-loader": "^0.23.1",
    "expect.js": "^0.3.1",
    "fs-extra": "^0.26.3",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "typedoc": "^0.3.11",
    "typescript": "^1.6.2",
    "webpack": "^1.12.9"
  },
  "scripts": {
    "clean": "rimraf lib && rimraf test/build && rimraf test/coverage",
    "clean:example": "rimraf example/bundle.js",
    "build:example": "webpack --config example/webpack.conf.js",
    "build:src": "tsc --project src",
    "build:css": "node scripts/copycss.js",
    "build": "npm run build:src && npm run build:css",
    "docs": "rimraf docs/api && typedoc --options scripts/tdoptions.json",
    "postinstall": "npm dedupe",
    "prepublish": "npm run build",
    "test:chrome": "karma start --browsers=Chrome test/karma.conf.js",
    "test:coverage": "karma start test/karma-cov.conf.js",
    "test:firefox": "karma start --browsers=Firefox test/karma.conf.js",
    "test:ie": "karma start --browsers=IE test/karma.conf.js",
    "test:debug": "karma start --browsers=Chrome --singleRun=false --debug=true test/karma.conf.js",
    "test": "npm run test:firefox"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Leftium/todo.taskpaper"
  },
  "files": [
    "lib/*.css",
    "lib/*.d.ts",
    "lib/*.js"
  ],
  "keywords": [
    "todo",
    "taskpaper"
  ],
  "author": "John-Kim Murphy <john@leftium.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Leftium/todo.taskpaper/issues"
  },
  "homepage": "https://github.com/Leftium/todo.taskpaper"
}
